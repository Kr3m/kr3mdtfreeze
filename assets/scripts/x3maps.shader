textures/X3maps_logo/lion01
{ 
    nopicmip
	qer_editorimage textures/X3maps_logo/lion01.tga
	surfaceparm trans	
//	surfaceparm alphashadow
	surfaceparm nomarks
	polygonOffset	
	cull none

       {
		map textures/X3maps_logo/lion01.tga
		blendFunc GL_ONE GL_ZERO
		alphaFunc GE128
		depthWrite
		rgbGen identity
       }
       {
		map $lightmap
		rgbGen identity
		blendFunc filter
		depthFunc equal
       }
}

